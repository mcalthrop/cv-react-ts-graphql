# EditorConfig: https://EditorConfig.org

# top-most EditorConfig file
root = true

# All files by default:
# - 2 space indentation
# - Unix-style newlines with a newline ending
# Add exceptions as a new rule where required.
[*]
indent_style = space
indent_size = 2
end_of_line = lf
insert_final_newline = true
